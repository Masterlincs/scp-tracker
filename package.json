{
  "name": "scp-tracker",
  "version": "1.0.0",
  "description": "Tracks which SCPs and tales you've read on the SCP Foundation wiki with enhanced features",
  "main": "background.js",
  "scripts": {
    "start": "node scripts/build.js chrome --watch",
    "build": "npm run clean && npm run build:chrome && npm run build:firefox && npm run build:firefox:mv2",
    "build:chrome": "node scripts/build.js chrome",
    "build:firefox": "node scripts/build.js firefox",
    "build:firefox:mv2": "node scripts/build.js firefox-mv2",
    "build:chrome:watch": "node scripts/build.js chrome --watch",
    "build:firefox:watch": "node scripts/build.js firefox --watch",
    "clean": "rimraf dist dist-chrome dist-firefox dist-firefox-mv2 coverage junit.xml",
    "clean:all": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "depcheck": "depcheck --json --ignores=@babel/*,eslint,eslint-*,prettier,husky,lint-staged,jest,jest-*,identity-obj-proxy",
    "depcheck:ci": "node scripts/depcheck-ci.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "scp",
    "extension",
    "tracker",
    "wiki",
    "reading",
    "productivity"
  ],
  "author": "masterlincs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Masterlincs/scp-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/Masterlincs/scp-tracker/issues"
  },
  "homepage": "https://github.com/Masterlincs/scp-tracker#readme",
  "dependencies": {
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.4.3",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "depcheck": "^1.4.7"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
